<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.2.xsd">
    
	<bean id="movieFinder1" class="com.luxoft.springioc.example22.MovieFinder">
		<property name="name" value="movieFinder1" />
	</bean>

	<bean id="movieFinder2" class="com.luxoft.springioc.example22.MovieFinder">
		<property name="name" value="movieFinder2" />
	</bean>

	<bean id="simpleMovieLister" class="com.luxoft.springioc.example22.SimpleMovieLister" />

	<context:annotation-config />

</beans>